{
  "name": "tradesignalapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "eslint",
    "db:seed": "tsx prisma/new-seed.ts",
    "db:migrate": "npx prisma migrate deploy && npm run db:seed",
    "db:reset": "prisma migrate reset --force && npm run db:seed"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@prisma/client": "^6.16.2",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.18",
    "@types/node": "^20.19.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-intl": "^4.3.9",
    "openai": "^5.22.0",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "eslint": "^9",
    "eslint-config-next": "15.5.3"
  }
}
